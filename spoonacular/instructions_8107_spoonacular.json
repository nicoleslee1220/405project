[{"name":"","steps":[{"number":1,"step":"Bring 3 cups water and piloncillo to boil in medium saucepan.","ingredients":[{"id":10099002,"name":"panela","localizedName":"panela","image":"panela.jpg"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"}]},{"number":2,"step":"Add white vinegar, canela, and salt; simmer until piloncillo has dissolved, stirring often, about 1 minute.","ingredients":[{"id":2053,"name":"distilled white vinegar","localizedName":"distilled white vinegar","image":"vinegar-(white).jpg"},{"id":10099002,"name":"panela","localizedName":"panela","image":"panela.jpg"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":3,"step":"Remove from heat.","ingredients":[],"equipment":[]},{"number":4,"step":"Add chiles and soak until softened, occasionally pressing to submerge, about 1 hour; drain. Pat chiles dry; slit lengthwise and remove seeds.","ingredients":[{"id":11819,"name":"chili pepper","localizedName":"chili pepper","image":"red-chili.jpg"},{"id":93818,"name":"seeds","localizedName":"seeds","image":"sunflower-seeds.jpg"}],"equipment":[],"length":{"number":60,"unit":"minutes"}},{"number":5,"step":"Cook red-skinned potatoes in pot of boiling salted water until skewer easily pierces centers, about 35 minutes.","ingredients":[{"id":10011355,"name":"red potato","localizedName":"red potato","image":"red-potatoes.jpg"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":3065,"name":"skewers","localizedName":"skewers","image":"wooden-skewers.jpg"},{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}],"length":{"number":35,"unit":"minutes"}},{"number":6,"step":"Drain. Cool, peel, and cut potatoes into 1-inch cubes.","ingredients":[{"id":11352,"name":"potato","localizedName":"potato","image":"potatoes-y