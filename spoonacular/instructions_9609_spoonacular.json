[{"name":"","steps":[{"number":1,"step":"Leave root and 1-inch stem on beets; scrub with a brush.","ingredients":[{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"}],"equipment":[]},{"number":2,"step":"Place in a medium saucepan; cover with water. Bring to a boil. Cover, reduce heat, and simmer 35 minutes or until tender.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"}],"length":{"number":35,"unit":"minutes"}},{"number":3,"step":"Drain and rinse with cold water.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[]},{"number":4,"step":"Drain; cool. Trim off beet roots; rub off skins.","ingredients":[{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"},{"id":1012034,"name":"dry seasoning rub","localizedName":"dry seasoning rub","image":"seasoning.png"}],"equipment":[]},{"number":5,"step":"Combine beets and 2 tablespoons juice in a food processor; process until smooth. Set aside.","ingredients":[{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"},{"id":1019016,"name":"juice","localizedName":"juice","image":"apple-juice.jpg"}],"equipment":[{"id":404771,"name":"food processor","localizedName":"food processor","image":"food-processor.png"}]},{"number":6,"step":"Position oven rack to lowest setting, and remove the middle rack. Preheat oven to 42","ingredients":[],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"}]},{"number":7,"step":"Place bread in a food processor, and process until finely ground. Lightly coat 8 (8-ounce) souffl dishes with cooking spray.","ingredients":[{"id":4679,"name":"cooking spray","localizedName":"cooking spray","image":"cooking-spray.png"},{"id":18064,"name":"bread","localizedName":"bread","image":"white-bread.jpg"}],"equipment":[{"id":404771,"name":"food processor","localize