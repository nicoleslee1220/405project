[{"name":"","steps":[{"number":1,"step":"Watch how to make this recipe.","ingredients":[],"equipment":[]},{"number":2,"step":"In a medium saute pan heat 3 tablespoons oil.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":3,"step":"Add the red onion, ginger, and garlic and saute for 3 minutes. Then add the shrimp, cook for 2 minutes, and then add the bok choy, cabbage, bell peppers. Cook for another 5 minutes to wilt. Deglaze with mirin. Toss in sesame oil, soy sauce, black pepper, salt and green onions.","ingredients":[{"id":10211821,"name":"bell pepper","localizedName":"bell pepper","image":"bell-pepper-orange.png"},{"id":1002030,"name":"black pepper","localizedName":"black pepper","image":"pepper.jpg"},{"id":11291,"name":"green onions","localizedName":"green onions","image":"spring-onions.jpg"},{"id":4058,"name":"sesame oil","localizedName":"sesame oil","image":"sesame-oil.png"},{"id":10011282,"name":"red onion","localizedName":"red onion","image":"red-onion.png"},{"id":16124,"name":"soy sauce","localizedName":"soy sauce","image":"soy-sauce.jpg"},{"id":11116,"name":"bok choy","localizedName":"bok choy","image":"bok-choy.jpg"},{"id":11109,"name":"cabbage","localizedName":"cabbage","image":"cabbage.jpg"},{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"},{"id":11216,"name":"ginger","localizedName":"ginger","image":"ginger.png"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"},{"id":93830,"name":"mirin","localizedName":"mirin","image":"mirin.jpg"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[],"length":{"number":10,"unit":"minutes"}},{"number":4,"step":"Let mixture cool in a bowl.","ingredients":[],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":5,"step":"Preheat Dutch oven with oil to 350 degrees F.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404667,"name":"dutch oven","localizedName":"dutch oven","image":"dutch-oven.jpg"}]},{"number":6,"step":"Mix tempura batter with ice cold water. (pancake batter consistency)","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":10014412,"name":"ice","localizedName":"ice","image":"ice-cubes.png"}],"equipment":[]},{"number":7,"step":"Hollow out 2 tablespoons of avocado from center, add 1/8th of cooled shrimp mixture to cavity of avocado, mixture should sit in avocado securely with only 1/2 to 1/4 of mixture standing out of avocado. Firmly press the mixture into the avocado. Holding the avocado with shrimp mixture, dip entire unit into batter and gently lower into the oil. Cook for 2 to 3 minutes or until golden brown. When cooked, remove and let drain on paper towels, serve hot.","ingredients":[{"id":9037,"name":"avocado","localizedName":"avocado","image":"avocado.jpg"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"},{"id":0,"name":"dip","localizedName":"dip","image":""},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":405895,"name":"paper towels","localizedName":"paper towels","image":"paper-towels.jpg"}],"length":{"number":2,"unit":"minutes"}},{"number":8,"step":"Drizzle with sweet chili sauce, and sprinkle with black sesame seeds.","ingredients":[{"id":10012023,"name":"black sesame seeds","localizedName":"black sesame seeds","image":"black-sesame-seeds-or-chia-seeds.png"},{"id":98962,"name":"sweet chili sauce","localizedName":"sweet chili sauce","image":"fish-sauce.jpg"}],"equipment":[]}]}]
