[{"name":"","steps":[{"number":1,"step":"In a small bowl or jar with a lid, make the spice blend.","ingredients":[{"id":1032027,"name":"seasoning mix","localizedName":"seasoning mix","image":"seasoning.png"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":2,"step":"Mix together the paprika, onion powder, garlic powder, white pepper, black pepper, cayenne pepper, oregano, thyme, celery seed and kosher salt. Coat the fish fillets with the spice mixture, and allow to sit at room temperature for no longer than 30 minutes.","ingredients":[{"id":2031,"name":"cayenne pepper","localizedName":"cayenne pepper","image":"chili-powder.jpg"},{"id":1022020,"name":"garlic powder","localizedName":"garlic powder","image":"garlic-powder.png"},{"id":1002030,"name":"black pepper","localizedName":"black pepper","image":"pepper.jpg"},{"id":10115261,"name":"fish fillets","localizedName":"fish fillets","image":"fish-fillet.jpg"},{"id":2026,"name":"onion powder","localizedName":"onion powder","image":"onion-powder.jpg"},{"id":2032,"name":"white pepper","localizedName":"white pepper","image":"white-pepper.png"},{"id":2007,"name":"celery seed","localizedName":"celery seed","image":"celery-seed.jpg"},{"id":1082047,"name":"kosher salt","localizedName":"kosher salt","image":"salt.jpg"},{"id":2027,"name":"oregano","localizedName":"oregano","image":"oregano.jpg"},{"id":2028,"name":"paprika","localizedName":"paprika","image":"paprika.jpg"},{"id":2049,"name":"thyme","localizedName":"thyme","image":"thyme.jpg"}],"equipment":[],"length":{"number":30,"unit":"minutes"}},{"number":3,"step":"Heat a heavy skillet over high heat.","ingredients":[],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":4,"step":"Add oil, and heat until it is almost smoking.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[]},{"number":5,"step":"Place the fillets in the pan, and cook for about 3 minutes per side, or until fish is opaque and can be flaked with a fork.","ingredients":[{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":3,"unit":"minutes"}},{"number":6,"step":"Remove from the pan, and place onto slices of white bread.","ingredients":[{"id":18069,"name":"white bread","localizedName":"white bread","image":"white-bread.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":7,"step":"Pour pan juices over them and squeeze lemon juice all over.  Do not underestimate the white bread. It gets quite tasty soaking up all the juices.","ingredients":[{"id":9152,"name":"lemon juice","localizedName":"lemon juice","image":"lemon-juice.jpg"},{"id":18069,"name":"white bread","localizedName":"white bread","image":"white-bread.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]}]}]
