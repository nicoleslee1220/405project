[{"name":"","steps":[{"number":1,"step":"Put the potatoes in a medium saucepan and cover with hot water. Cook over high heat until tender, about 12 minutes.","ingredients":[{"id":11352,"name":"potato","localizedName":"potato","image":"potatoes-yukon-gold.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"}],"length":{"number":12,"unit":"minutes"}},{"number":2,"step":"Meanwhile, heat the oil in a large, heavy casserole.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[]},{"number":3,"step":"Add the chicken and cook over high heat until lightly browned, 4 minutes.","ingredients":[{"id":5006,"name":"whole chicken","localizedName":"whole chicken","image":"whole-chicken.jpg"}],"equipment":[],"length":{"number":4,"unit":"minutes"}},{"number":4,"step":"Add the onion and jalapeo and cook, stirring, until the onion is softened, 4 minutes.","ingredients":[{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[],"length":{"number":4,"unit":"minutes"}},{"number":5,"step":"Add 3 tablespoons of the fish sauce and the stock and bring to a boil. Cover and cook over moderate heat until the chicken is just cooked through, about 10 minutes.","ingredients":[{"id":6179,"name":"fish sauce","localizedName":"fish sauce","image":"asian-fish-sauce.jpg"},{"id":5006,"name":"whole chicken","localizedName":"whole chicken","image":"whole-chicken.jpg"},{"id":1006615,"name":"stock","localizedName":"stock","image":"chicken-broth.png"}],"equipment":[],"length":{"number":10,"unit":"minutes"}},{"number":6,"step":"Using a slotted spoon, transfer the potatoes to a ricer and press into a bowl. (Alternatively, mash the potatoes.) Reserve the potato water.","ingredients":[{"id":11352,"name":"potato","localizedName":"potato","image":"potatoes-yukon-gold.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404636,"name":"slotted spoon","localizedName":"slotted spoon","image":"slotted-spoon.jpg"},{"id":404790,"name":"potato ricer","localizedName":"potato ricer","image":"potato-ricer.jpg"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":7,"step":"Add the 2 tablespoons of flour, egg yolk, chives and 1/2 teaspoon of salt to the potatoes and stir until a stiff dough forms. Turn the dough out onto a heavily floured board and divide it in half.","ingredients":[{"id":1125,"name":"egg yolk","localizedName":"egg yolk","image":"egg-yolk.jpg"},{"id":11352,"name":"potato","localizedName":"potato","image":"potatoes-yukon-gold.png"},{"id":11156,"name":"chives","localizedName":"chives","image":"fresh-chives.jpg"},{"id":0,"name":"dough","localizedName":"dough","image":"pizza-dough"},{"id":20081,"name":"all purpose flour","localizedName":"all purpose flour","image":"flour.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[]},{"number":8,"step":"Roll each piece into a 1/2-inch-thick rope.","ingredients":[{"id":0,"name":"roll","localizedName":"roll","image":"dinner-yeast-rolls.jpg"}],"equipment":[]},{"number":9,"step":"Cut the ropes into 1-inch pieces.","ingredients":[],"equipment":[]},{"number":10,"step":"Add the bok choy to the stew and cook until crisp-tender, about 5 minutes.","ingredients":[{"id":11116,"name":"bok choy","localizedName":"bok choy","image":"bok-choy.jpg"},{"id":0,"name":"stew","localizedName":"stew","image":""}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":11,"step":"Add the cornstarch mixture and cook, stirring, until thickened, 1 minute.","ingredients":[{"id":20027,"name":"corn starch","localizedName":"corn starch","image":"white-powder.jpg"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":12,"step":"Return the potato water to a boil and add the dumplings. Cook over high heat until they rise to the surface, then simmer for  2 minutes. Using a slotted spoon, transfer to the chicken stew.","ingredients":[{"id":5006,"name":"whole chicken","localizedName":"whole chicken","image":"whole-chicken.jpg"},{"id":11352,"name":"potato","localizedName":"potato","image":"potatoes-yukon-gold.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":0,"name":"stew","localizedName":"stew","image":""}],"equipment":[{"id":404636,"name":"slotted spoon","localizedName":"slotted spoon","image":"slotted-spoon.jpg"}],"length":{"number":2,"unit":"minutes"}},{"number":13,"step":"Add the basil and the remaining 1 tablespoon of fish sauce and simmer the stew for 2 to 3 minutes.","ingredients":[{"id":6179,"name":"fish sauce","localizedName":"fish sauce","image":"asian-fish-sauce.jpg"},{"id":2044,"name":"basil","localizedName":"basil","image":"basil.jpg"},{"id":0,"name":"stew","localizedName":"stew","image":""}],"equipment":[],"length":{"number":2,"unit":"minutes"}},{"number":14,"step":"Serve with lime wedges.","ingredients":[{"id":1029159,"name":"lime wedge","localizedName":"lime wedge","image":"lime-wedge.jpg"}],"equipment":[]}]}]
