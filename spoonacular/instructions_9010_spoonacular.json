[{"name":"","steps":[{"number":1,"step":"In a small bowl combine the aioli ingredients. Refrigerate until ready to assemble the sandwiches.","ingredients":[{"id":93758,"name":"aioli","localizedName":"aioli","image":"aioli.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":2,"step":"In a small bowl whisk the marinade ingredients.","ingredients":[{"id":0,"name":"marinade","localizedName":"marinade","image":"seasoning.png"}],"equipment":[{"id":404661,"name":"whisk","localizedName":"whisk","image":"whisk.png"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":3,"step":"Place the mushroom caps, gill sides down, in a large baking pan.","ingredients":[{"id":11260,"name":"mushrooms","localizedName":"mushrooms","image":"mushrooms.png"}],"equipment":[{"id":404646,"name":"baking pan","localizedName":"baking pan","image":"roasting-pan.jpg"}]},{"number":4,"step":"Brush the mushroom caps generously with the marinade and turn the caps over. Spoon the rest of the marinade over the gill side. Allow the mushrooms to marinate at room temperature for 15 to 20 minutes.","ingredients":[{"id":11260,"name":"mushrooms","localizedName":"mushrooms","image":"mushrooms.png"},{"id":0,"name":"marinade","localizedName":"marinade","image":"seasoning.png"}],"equipment":[],"length":{"number":15,"unit":"minutes"}},{"number":5,"step":"Prepare the grill for direct cooking over medium heat.","ingredients":[],"equipment":[{"id":404706,"name":"grill","localizedName":"grill","image":"grill.jpg"}]},{"number":6,"step":"Remove the mushrooms from the pan and reserve the marinade. Lightly season the mushrooms with salt and pepper.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":11260,"name":"mushrooms","localizedName":"mushrooms","image":"mushrooms.png"},{"id":0,"name":"marinade","localizedName":"marinade","image":"seasoning.png"}],"equipment":[{"id":404