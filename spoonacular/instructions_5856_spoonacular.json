[{"name":"","steps":[{"number":1,"step":"Preheat oven to 450F and line a baking sheet with parchment paper.To make the coconut chutney, bring pineapple juice to a boil in a small saucepan.","ingredients":[{"id":9273,"name":"pineapple juice","localizedName":"pineapple juice","image":"pineapple-juice.jpg"},{"id":0,"name":"chutney","localizedName":"chutney","image":""},{"id":12104,"name":"coconut","localizedName":"coconut","image":"coconut.jpg"}],"equipment":[{"id":404770,"name":"baking paper","localizedName":"baking paper","image":"baking-paper.jpg"},{"id":404727,"name":"baking sheet","localizedName":"baking sheet","image":"baking-sheet.jpg"},{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"},{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg","temperature":{"number":450.0,"unit":"Fahrenheit"}}]},{"number":2,"step":"Remove from heat and stir in coconut, cumin, coriander, ginger and red pepper flakes. Cover and let rest for 20 minutes to allow coconut to absorb liquid. Stir in cilantro. Set aside until ready to serve.For the shrimp, rinse them and pat dry. Pulse macadamia nuts in a food processor until finely ground.","ingredients":[{"id":1032009,"name":"red pepper flakes","localizedName":"red pepper flakes","image":"red-pepper-flakes.jpg"},{"id":12131,"name":"macadamia nuts","localizedName":"macadamia nuts","image":"macadamia-nuts.png"},{"id":1012013,"name":"coriander","localizedName":"coriander","image":"ground-coriander.jpg"},{"id":11165,"name":"cilantro","localizedName":"cilantro","image":"cilantro.png"},{"id":12104,"name":"coconut","localizedName":"coconut","image":"coconut.jpg"},{"id":11216,"name":"ginger","localizedName":"ginger","image":"ginger.png"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"},{"id":1002014,"name":"cumin","localizedName":"cumin","image":"ground-cumin.jpg"}],"equipment":[{"id":404771,"name":"food processor","localizedName":"food processor","image":"food-processor.png"}],"length":{"number":20,"unit":"minutes"}},{"number":3,"step":"Transfer nuts to a medium bowl and stir in panko, salt and pepper.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":10018079,"name":"panko","localizedName":"panko","image":"panko.jpg"},{"id":12135,"name":"nuts","localizedName":"nuts","image":"nuts-mixed.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":4,"step":"Spread flour in a shallow plate and pour eggs into a shallow plate. Holding a shrimp by the tail, dip in the flour and shake off excess. Next, dip floured shrimp in the egg. Finally dip shrimp in the nut mixture, pressing to coat evenly.Arrange shrimp on the prepared baking sheet and bake until crisp and lightly browned, 6 to 10 minutes. Alternatively, shallow pan-fry in canola oil for 1 to 2 minutes per side.","ingredients":[{"id":1014582,"name":"canola oil","localizedName":"canola oil","image":"vegetable-oil.jpg"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"},{"id":0,"name":"spread","localizedName":"spread","image":""},{"id":20081,"name":"all purpose flour","localizedName":"all purpose flour","image":"flour.png"},{"id":0,"name":"shake","localizedName":"shake","image":""},{"id":1123,"name":"egg","localizedName":"egg","image":"egg.png"},{"id":0,"name":"dip","localizedName":"dip","image":""}],"equipment":[{"id":404727,"name":"baking sheet","localizedName":"baking sheet","image":"baking-sheet.jpg"},{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"},{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":7,"unit":"minutes"}},{"number":5,"step":"Serve hot with coconut chutney.","ingredients":[{"id":0,"name":"chutney","localizedName":"chutney","image":""},{"id":12104,"name":"coconut","localizedName":"coconut","image":"coconut.jpg"}],"equipment":[]}]}]
