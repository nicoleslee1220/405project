[{"name":"","steps":[{"number":1,"step":"In a small saucepan, combine the vinegar, sugar, fish sauce, and 1/8 tsp. salt; stir over low heat until the sugar dissolves. Set aside to cool slightly. In a large bowl, combine the cabbage, carrots, bell pepper, and scallions; toss well.","ingredients":[{"id":10211821,"name":"bell pepper","localizedName":"bell pepper","image":"bell-pepper-orange.png"},{"id":6179,"name":"fish sauce","localizedName":"fish sauce","image":"asian-fish-sauce.jpg"},{"id":11291,"name":"green onions","localizedName":"green onions","image":"spring-onions.jpg"},{"id":11109,"name":"cabbage","localizedName":"cabbage","image":"cabbage.jpg"},{"id":11124,"name":"carrot","localizedName":"carrot","image":"sliced-carrot.png"},{"id":2053,"name":"vinegar","localizedName":"vinegar","image":"vinegar-(white).jpg"},{"id":19335,"name":"sugar","localizedName":"sugar","image":"sugar-in-bowl.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":2,"step":"Pour the vinegar mixture over the cabbage and mix well to combine. Toss in the peanuts.","ingredients":[{"id":11109,"name":"cabbage","localizedName":"cabbage","image":"cabbage.jpg"},{"id":16091,"name":"peanuts","localizedName":"peanuts","image":"peanuts.png"},{"id":2053,"name":"vinegar","localizedName":"vinegar","image":"vinegar-(white).jpg"}],"equipment":[]},{"number":3,"step":"Let sit, stirring occasionally, for about 20 minutes.    In a large nonstick skillet, heat 1 Tbs. of the sesame oil over medium-high heat. Toss the shrimp with 1/2 tsp. salt and 1/4 tsp. pepper. Put half the shrimp in the skillet and saut until opaque throughout, about 3 minutes.","ingredients":[{"id":4058,"name":"sesame oil","localizedName":"sesame oil","image":"sesame-oil.png"},{"id":1002030,"name":"pepper","localizedName":"pepper","image":"pepper.jpg"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":23,"unit":"minutes"}},{"number":4,"step":"Transfer the cooked shrimp to a clean bowl. Repeat with the remaining 1 Tbs. oil and the other half of the shrimp. Toss the shrimp into the slaw and serve.","ingredients":[{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]}]},{"name":"nutrition information (per serving)","steps":[{"number":1,"step":"Size","ingredients":[],"equipment":[]},{"number":2,"step":"based on ten servings, Calories","ingredients":[],"equipment":[]}]},{"name":"(kcal)","steps":[{"number":1,"step":"170, Fat","ingredients":[],"equipment":[]},{"number":2,"step":"6, Fat Calories","ingredients":[],"equipment":[]}]},{"name":"(g)","steps":[{"number":1,"step":"60, Saturated Fat","ingredients":[],"equipment":[]},{"number":2,"step":"1, Protein","ingredients":[],"equipment":[]},{"number":3,"step":"17, Monounsaturated Fat","ingredients":[],"equipment":[]},{"number":4,"step":"0, Carbohydrates","ingredients":[],"equipment":[]},{"number":5,"step":"620, Cholesterol","ingredients":[],"equipment":[]},{"number":6,"step":"135, Fiber","ingredients":[],"equipment":[]},{"number":7,"step":"Photo: Scott Phillips","ingredients":[],"equipment":[]},{"number":8,"step":"Rate this Recipe and View Reviews","ingredients":[],"equipment":[]}]},{"name":"(mg)","steps":[{"number":1,"step":"9, Polyunsaturated Fat","ingredients":[],"equipment":[]},{"number":2,"step":"0, Sodium","ingredients":[],"equipment":[]}]}]
