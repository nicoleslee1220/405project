[{"name":"","steps":[{"number":1,"step":"Bake nuts in a 9-inch pie or cake pan in a 350 regular or convection oven until golden beneath skins, shaking pan once, 7 to 9 minutes.","ingredients":[{"id":12135,"name":"nuts","localizedName":"nuts","image":"nuts-mixed.jpg"}],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"},{"id":404747,"name":"cake form","localizedName":"cake form","image":"cake-pan.png"}],"length":{"number":7,"unit":"minutes"}},{"number":2,"step":"Meanwhile, in a 10- to 12-inch frying pan over high heat, bring about 1 inch water to a boil. Rinse asparagus and snap off tough stem ends.","ingredients":[{"id":11011,"name":"asparagus","localizedName":"asparagus","image":"asparagus.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":3,"step":"Add asparagus or broccoli to pan and boil, uncovered, until bright green and barely tender when pierced, 2 to 3 minutes.","ingredients":[{"id":11011,"name":"asparagus","localizedName":"asparagus","image":"asparagus.png"},{"id":11090,"name":"broccoli","localizedName":"broccoli","image":"broccoli.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":2,"unit":"minutes"}},{"number":4,"step":"Drain; rinse with cold water until cold.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[]},{"number":5,"step":"Rinse salmon and pat dry. Coat flesh sides equally with all the spice rub.","ingredients":[{"id":1012034,"name":"dry seasoning rub","localizedName":"dry seasoning rub","image":"seasoning.png"},{"id":15076,"name":"salmon","localizedName":"salmon","image":"salmon.png"}],"equipment":[]},{"number":6,"step":"Lay fish, coated side down, on a grill over hot coals or high heat on a gas grill (you can hold your hand at grill level only 2 to 3 seconds); close lid on gas grill. Cook fish, turning once, until opaque but still moist-looking in center of thickest part (cut to test), 7 to 9 minutes total.","ingredients":[{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404706,"name":"grill","localizedName":"grill","image":"grill.jpg"}],"length":{"number":7,"unit":"minutes"}},{"number":7,"step":"Transfer to a plate.","ingredients":[],"equipment":[]},{"number":8,"step":"Rinse Belgian endive; trim off and discard discolored ends. Set aside 12 leaves; cut remaining leaves crosswise into 1/4-inch-wide slices and place in a bowl.","ingredients":[{"id":11151,"name":"belgian endive","localizedName":"belgian endive","image":"chicory.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":9,"step":"Add salad greens, tomatoes, onion, and 1/4 cup raspberry vinaigrette; mix gently.","ingredients":[{"id":98952,"name":"raspberry vinaigrette","localizedName":"raspberry vinaigrette","image":"raspberry-vinaigrette.png"},{"id":21052,"name":"greens","localizedName":"greens","image":"mixed-greens-or-mesclun.jpg"},{"id":11529,"name":"tomato","localizedName":"tomato","image":"tomato.png"},{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[]},{"number":10,"step":"Arrange asparagus and whole endive leaves equally around edges of plates. Mound salad mixture equally in center of plates; top with warm salmon.","ingredients":[{"id":11011,"name":"asparagus","localizedName":"asparagus","image":"asparagus.png"},{"id":11213,"name":"endive","localizedName":"endive","image":"escarole.jpg"},{"id":15076,"name":"salmon","localizedName":"salmon","image":"salmon.png"}],"equipment":[]},{"number":11,"step":"Sprinkle servings equally with walnuts, raspberries, and soybeans.","ingredients":[{"id":9302,"name":"raspberries","localizedName":"raspberries","image":"raspberries.jpg"},{"id":16409,"name":"soybeans","localizedName":"soybeans","image":"soy-beans.png"},{"id":12155,"name":"walnuts","localizedName":"walnuts","image":"walnuts.jpg"}],"equipment":[]},{"number":12,"step":"Drizzle remaining vinaigrette over the top.","ingredients":[{"id":4135,"name":"vinaigrette","localizedName":"vinaigrette","image":"vinaigrette.jpg"}],"equipment":[]},{"number":13,"step":"Serve with pumpernickel rolls.","ingredients":[{"id":18044,"name":"pumpernickel","localizedName":"pumpernickel","image":"pumpernickel.jpg"},{"id":0,"name":"roll","localizedName":"roll","image":"dinner-yeast-rolls.jpg"}],"equipment":[]},{"number":14,"step":"Spice rub: In a blender, whirl 1 tablespoon each coriander seeds and fennel seeds and 1 1/2 teaspoons each dried thyme and black peppercorns until finely ground. Makes about 3 tablespoons.","ingredients":[{"id":1022030,"name":"black peppercorns","localizedName":"black peppercorns","image":"black-pepper.png"},{"id":2013,"name":"whole coriander seeds","localizedName":"whole coriander seeds","image":"coriander-seeds.jpg"},{"id":2018,"name":"fennel seeds","localizedName":"fennel seeds","image":"fennel-seeds.jpg"},{"id":2042,"name":"dried thyme","localizedName":"dried thyme","image":"thyme.jpg"},{"id":1012034,"name":"dry seasoning rub","localizedName":"dry seasoning rub","image":"seasoning.png"}],"equipment":[{"id":404726,"name":"blender","localizedName":"blender","image":"blender.png"}]},{"number":15,"step":"Raspberry vinaigrette: In a small bowl, whisk together 3 tablespoons raspberry vinegar, 2 tablespoons extra-virgin olive oil, 1 tablespoon walnut oil (optional), and 1 1/2 teaspoons Dijon mustard.","ingredients":[{"id":1034053,"name":"extra virgin olive oil","localizedName":"extra virgin olive oil","image":"olive-oil.jpg"},{"id":98952,"name":"raspberry vinaigrette","localizedName":"raspberry vinaigrette","image":"raspberry-vinaigrette.png"},{"id":1002048,"name":"raspberry vinegar","localizedName":"raspberry vinegar","image":"red-wine-vinegar.jpg"},{"id":1032046,"name":"dijon mustard","localizedName":"dijon mustard","image":"dijon-mustard.jpg"},{"id":4528,"name":"walnut oil","localizedName":"walnut oil","image":"walnut-oil.jpg"}],"equipment":[{"id":404661,"name":"whisk","localizedName":"whisk","image":"whisk.png"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":16,"step":"Add salt and pepper to taste. Makes about 1/2 cup.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"}],"equipment":[]}]}]
