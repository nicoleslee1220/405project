[{"name":"","steps":[{"number":1,"step":"Cut tofu into 16 rectangles shaped like husky dominoes (each about 1 in. wide, 1 1/2 in. long, and 1 in. thick). Put tofu in a wide, shallow bowl.","ingredients":[{"id":16213,"name":"tofu","localizedName":"tofu","image":"tofu.png"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":2,"step":"Mix 1 tsp. salt with 2 cups very hot or just-boiled water and pour over tofu to just cover.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"},{"id":16213,"name":"tofu","localizedName":"tofu","image":"tofu.png"}],"equipment":[]},{"number":3,"step":"Let sit 15 minutes.","ingredients":[],"equipment":[],"length":{"number":15,"unit":"minutes"}},{"number":4,"step":"Transfer tofu to a double layer of paper towels set on a plate and let drain 10 minutes.","ingredients":[{"id":16213,"name":"tofu","localizedName":"tofu","image":"tofu.png"}],"equipment":[{"id":405895,"name":"paper towels","localizedName":"paper towels","image":"paper-towels.jpg"}],"length":{"number":10,"unit":"minutes"}},{"number":5,"step":"Meanwhile, heat 2 tbsp. oil in a medium nonstick frying pan over medium heat. Fry shallots, stirring occasionally, until light golden, 9 to 10 minutes.","ingredients":[{"id":11677,"name":"shallot","localizedName":"shallot","image":"shallots.jpg"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":9,"unit":"minutes"}},{"number":6,"step":"Add garlic and curry leaves and fry 2 to 3 minutes, stirring constantly, until light and crisp.","ingredients":[{"id":93604,"name":"curry leaves","localizedName":"curry leaves","image":"curry-leaves.jpg"},{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"}],"equipment":[],"length":{"number":2,"unit":"minutes"}},{"number":7,"step":"Add curry powder, remove from heat, and stir to aromatize spices. Using a slotted spoon or skimmer, transfer seasonings to a bowl, leaving most of fragrant oil in pan.","ingredients":[{"id":2015,"name":"curry powder","localizedName":"curry powder","image":"curry-powder.jpg"},{"id":1042027,"name":"seasoning","localizedName":"seasoning","image":"seasoning.png"},{"id":2035,"name":"spices","localizedName":"spices","image":"spices.png"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404636,"name":"slotted spoon","localizedName":"slotted spoon","image":"slotted-spoon.jpg"},{"id":405894,"name":"skimmer","localizedName":"skimmer","image":"skimmer-spoon.jpg"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"},{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":8,"step":"Blot tofu dry.","ingredients":[{"id":16213,"name":"tofu","localizedName":"tofu","image":"tofu.png"}],"equipment":[]},{"number":9,"step":"Add more oil to pan, if needed, to film bottom and heat to medium-high. Fry tofu, turning once, until golden, 4 to 5 minutes (it won't be crisp).","ingredients":[{"id":16213,"name":"tofu","localizedName":"tofu","image":"tofu.png"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":4,"unit":"minutes"}},{"number":10,"step":"Lower heat slightly and return all but 1 tbsp. shallot mixture to pan.","ingredients":[{"id":11677,"name":"shallot","localizedName":"shallot","image":"shallots.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":11,"step":"Add coconut milk, bamboo shoots, and palm sugar. Bring to a simmer and cook 2 to 3 minutes.","ingredients":[{"id":11028,"name":"bamboo shoots","localizedName":"bam