[{"name":"","steps":[{"number":1,"step":"Preheat the oven to 450F.","ingredients":[],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg","temperature":{"number":450.0,"unit":"Fahrenheit"}}]},{"number":2,"step":"Bring a large pot of water to a vigorous boil, add 1/2 teaspoon of the salt, then add the carrots. Blanch for 1 minute and remove to a bowl of ice water to stop the cooking. Once cool, remove and set aside.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":11124,"name":"carrot","localizedName":"carrot","image":"sliced-carrot.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"},{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}],"length":{"number":1,"unit":"minutes"}},{"number":3,"step":"Place the beets in a large pot of cold water. Bring to a boil, add the remaining 1/2 teaspoon of salt, and simmer until the beets are tender. Strain the beets and peel them using paper towels to rub off the skin. This is easier when they are still warm.","ingredients":[{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"},{"id":1012034,"name":"dry seasoning rub","localizedName":"dry seasoning rub","image":"seasoning.png"}],"equipment":[{"id":405895,"name":"paper towels","localizedName":"paper towels","image":"paper-towels.jpg"},{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":4,"step":"Crumble the feta and set aside.","ingredients":[{"id":1019,"name":"feta cheese","localizedName":"feta cheese","image":"feta.png"}],"equipment":[]},{"number":5,"step":"Toss the carrots with 1/2 tablespoon of the olive oil and place them on half of a rimmed baking sheet. Toss the beets with the remaining olive oil and place on the other half of the baking sheet. Roast for 15 minutes.","ingredients":[{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"},{"id":11124,"name":"carrot","localizedName":"carrot","image":"sliced-carrot.png"},{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"}],"equipment":[{"id":404727,"name":"baking sheet","localizedName":"baking sheet","image":"baking-sheet.jpg"}],"length":{"number":15,"unit":"minutes"}},{"number":6,"step":"Remove the beets and carrots from the oven and place in separate bowls.","ingredients":[{"id":11124,"name":"carrot","localizedName":"carrot","image":"sliced-carrot.png"},{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"},{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"}]},{"number":7,"step":"Add 1 tablespoon of the vinaigrette and 1/2 cup of the parsley to the beets and toss.","ingredients":[{"id":4135,"name":"vinaigrette","localizedName":"vinaigrette","image":"vinaigrette.jpg"},{"id":11297,"name":"parsley","localizedName":"parsley","image":"parsley.jpg"},{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"}],"equipment":[]},{"number":8,"step":"Add 1 tablespoon of the vinaigrette, the remaining parsley, and the feta to the carrots and toss. Divide the carrots evenly among 6 plates. Then divide the beets evenly among the plates and gently mix with the carrots.","ingredients":[{"id":4135,"name":"vinaigrette","localizedName":"vinaigrette","image":"vinaigrette.jpg"},{"id":11124,"name":"carrot","localizedName":"carrot","image":"sliced-carrot.png"},{"id":11297,"name":"parsley","localizedName":"parsley","image":"parsley.jpg"},{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"},{"id":1019,"name":"feta cheese","localizedName":"feta cheese","image":"feta.png"}],"equipment":[]},{"number":9,"step":"Drizzle with a touch more of the vinaigrette.","ingredients":[{"id":4135,"name":"vinaigrette","localizedName":"vinaigrette","image":"vinaigrette.jpg"}],"equipment":[]}]}]
