[{"name":"","steps":[{"number":1,"step":"Plump (soak) the raisins in  cups of the milk for 30 minutes.","ingredients":[{"id":9299,"name":"raisins","localizedName":"raisins","image":"raisins.jpg"},{"id":1077,"name":"milk","localizedName":"milk","image":"milk.png"}],"equipment":[],"length":{"number":30,"unit":"minutes"}},{"number":2,"step":"Drain, reserving the milk.","ingredients":[{"id":1077,"name":"milk","localizedName":"milk","image":"milk.png"}],"equipment":[]},{"number":3,"step":"Combine the yeast, whole wheat flour, and remaining I cup milk.","ingredients":[{"id":20080,"name":"whole wheat flour","localizedName":"whole wheat flour","image":"flour.png"},{"id":18375,"name":"yeast","localizedName":"yeast","image":"yeast-granules.jpg"},{"id":1077,"name":"milk","localizedName":"milk","image":"milk.png"}],"equipment":[]},{"number":4,"step":"Let stand for 30 minutes until very foamy. Meanwhile, bring 2 cups water to a boil in a small saucepan.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"}],"length":{"number":30,"unit":"minutes"}},{"number":5,"step":"Add the quinoa and amaranth and simmer, uncovered, until the grains are tender, about 10 minutes.","ingredients":[{"id":20001,"name":"amaranth","localizedName":"amaranth","image":"amaranth.jpg"},{"id":0,"name":"grains","localizedName":"grains","image":""},{"id":20035,"name":"quinoa","localizedName":"quinoa","image":"uncooked-quinoa.png"}],"equipment":[],"length":{"number":10,"unit":"minutes"}},{"number":6,"step":"Remove from heat and let stand covered until the water is completely absorbed, 10 minutes; cool.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[],"length":{"number":10,"unit":"minutes"}},{"number":7,"step":"Combine the bread flour, spelt flour, brown sugar, and salt in a mixing bowl.","ingredients":[{"id":10120129,"name":"