[{"name":"","steps":[{"number":1,"step":"Preheat the oven to 425 degrees F.Season the snappers with the salt and pepper.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":15101,"name":"snapper","localizedName":"snapper","image":"snapper.jpg"}],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg","temperature":{"number":425.0,"unit":"Fahrenheit"}}]},{"number":2,"step":"Place the orange slices on the bottom of a large roasting pan in 2 columns of 4 orange slices.","ingredients":[{"id":1019200,"name":"orange slices","localizedName":"orange slices","image":"orange-slices.png"}],"equipment":[{"id":404629,"name":"roasting pan","localizedName":"roasting pan","image":"roasting-pan.jpg"}]},{"number":3,"step":"Place 3 sprigs of rosemary across the each of the 2 rows of oranges.","ingredients":[{"id":2036,"name":"rosemary","localizedName":"rosemary","image":"rosemary.jpg"},{"id":9200,"name":"orange","localizedName":"orange","image":"orange.png"}],"equipment":[]},{"number":4,"step":"Lay the fish on top of the orange slices, and divide the rosemary leaves, orange zest, garlic cloves, and olives evenly over the tops of the 2 fish.","ingredients":[{"id":2036,"name":"rosemary","localizedName":"rosemary","image":"rosemary.jpg"},{"id":10211215,"name":"whole garlic cloves","localizedName":"whole garlic cloves","image":"garlic.jpg"},{"id":1019200,"name":"orange slices","localizedName":"orange slices","image":"orange-slices.png"},{"id":9216,"name":"orange zest","localizedName":"orange zest","image":"orange-zest.png"},{"id":9195,"name":"olives","localizedName":"olives","image":"olives-mixed.jpg"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[]},{"number":5,"step":"Pour the wine and orange juice around the fish, and drizzle each fish with 2 tablespoons of the olive oil.","ingredients":[{"id":9206,"name":"orange juice","localizedName":"orange juice","image":"orange-juice.jpg"},{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"},{"id":14084,"name":"wine","localizedName":"wine","image":"red-wine.jpg"}],"equipment":[]},{"number":6,"step":"Place the fish in the oven and roast, uncovered, for 20 minutes.","ingredients":[{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"}],"length":{"number":20,"unit":"minutes"}},{"number":7,"step":"Remove from the oven and add 2 tablespoons of olive oil over each fish. Return to the oven and continue to cook until well caramelized, about 15 to 20 minutes.","ingredients":[{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"}],"length":{"number":15,"unit":"minutes"}},{"number":8,"step":"Remove the fish from the oven and drizzle with the remaining 1/4 cup of olive oil.","ingredients":[{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"}]},{"number":9,"step":"Serve immediately, with the pan drippings drizzled over the top of the fish.","ingredients":[{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]}]}]
