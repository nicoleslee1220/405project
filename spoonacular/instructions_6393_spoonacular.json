[{"name":"","steps":[{"number":1,"step":"Bring a medium saucepan of water to a boil. Prepare a medium bowl of ice water.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":2,"step":"Add the Old Bay seasoning and the shrimp and scallops to the boiling water and cook for 1 minute. With a slotted spoon, transfer the shrimp and scallops to the ice water bath.","ingredients":[{"id":1052034,"name":"old bay seasoning","localizedName":"old bay seasoning","image":"seasoning.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":10015172,"name":"scallops","localizedName":"scallops","image":"scallops.jpg"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"}],"equipment":[{"id":404636,"name":"slotted spoon","localizedName":"slotted spoon","image":"slotted-spoon.jpg"}],"length":{"number":1,"unit":"minutes"}},{"number":3,"step":"Add the squid to the boiling water and cook for 5 seconds, then transfer to the ice water bath.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":15175,"name":"squid","localizedName":"squid","image":"calamari-or-squid.jpg"}],"equipment":[]},{"number":4,"step":"Add the clams to the saucepan and boil until they open, then transfer them to a separate bowl.","ingredients":[{"id":15157,"name":"clams","localizedName":"clams","image":"clams.jpg"}],"equipment":[{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":5,"step":"Remove the clams from their shells.","ingredients":[{"id":11020420,"name":"pasta shells","localizedName":"pasta shells","image":"shell-pasta.jpg"},{"id":15157,"name":"clams","localizedName":"clams","image":"clams.jpg"}],"equipment":[]},{"number":6,"step":"Drain the shrimp, scallops and squid and pat dry. In a large, shallow dish, combine the shrimp, scallops, squid, clams and snapper.","ingredients":[{"id":10015172,"name":"scallops","localizedName":"scallops","image":"scallops.jpg"},{"id":15101,"name":"snapper","localizedName":"snapper","image":"snapper.jpg"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"},{"id":15157,"name":"clams","localizedName":"clams","image":"clams.jpg"},{"id":15175,"name":"squid","localizedName":"squid","image":"calamari-or-squid.jpg"}],"equipment":[]},{"number":7,"step":"Sprinkle with the salt, ginger, celery and habanero and toss. Refrigerate for 30 minutes.","ingredients":[{"id":10011819,"name":"habanero chili","localizedName":"habanero chili","image":"habanero-pepper.jpg"},{"id":11143,"name":"celery","localizedName":"celery","image":"celery.jpg"},{"id":11216,"name":"ginger","localizedName":"ginger","image":"ginger.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[],"length":{"number":30,"unit":"minutes"}},{"number":8,"step":"Add the lime juice, cilantro and onion to the seviche. Season with pepper and toss well. Refrigerate for 2 hours, stirring twice. Spoon the seviche into martini glasses and serve.","ingredients":[{"id":9160,"name":"lime juice","localizedName":"lime juice","image":"lime-juice.png"},{"id":11165,"name":"cilantro","localizedName":"cilantro","image":"cilantro.png"},{"id":0,"name":"martini","localizedName":"martini","image":"rum-dark.jpg"},{"id":1002030,"name":"pepper","localizedName":"pepper","image":"pepper.jpg"},{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[],"length":{"number":120,"unit":"minutes"}}]}]
