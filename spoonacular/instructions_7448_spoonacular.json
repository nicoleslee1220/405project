[{"name":"","steps":[{"number":1,"step":"Special equipment: 4 large sheets of aluminum foil or parchment paper","ingredients":[],"equipment":[{"id":404770,"name":"baking paper","localizedName":"baking paper","image":"baking-paper.jpg"},{"id":404765,"name":"aluminum foil","localizedName":"aluminum foil","image":"aluminum-foil.png"}]},{"number":2,"step":"Preheat the oven to 350 degrees F. In a small bowl mix together the lemon zest, salt, and pepper. Set aside.","ingredients":[{"id":9156,"name":"lemon zest","localizedName":"lemon zest","image":"zest-lemon.jpg"},{"id":1002030,"name":"pepper","localizedName":"pepper","image":"pepper.jpg"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"},{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg","temperature":{"number":350.0,"unit":"Fahrenheit"}}]},{"number":3,"step":"Lay out the 4 sheets of aluminum foil.","ingredients":[],"equipment":[{"id":404765,"name":"aluminum foil","localizedName":"aluminum foil","image":"aluminum-foil.png"}]},{"number":4,"step":"Place 1/4 of the sugar snap peas, 1/4 of the yellow bell peppers, and 1/4 of the orange bell peppers on each sheet of foil. Over each pile of vegetables drizzle 2 tablespoons of white wine, 1 tablespoon of lemon juice, and 1 teaspoon of olive oil.","ingredients":[{"id":10011821,"name":"orange pepper","localizedName":"orange pepper","image":"bell-pepper-orange.png"},{"id":11951,"name":"yellow pepper","localizedName":"yellow pepper","image":"yellow-bell-pepper.jpg"},{"id":10011300,"name":"sugar snap peas","localizedName":"sugar snap peas","image":"sugar-snap-peas.jpg"},{"id":9152,"name":"lemon juice","localizedName":"lemon juice","image":"lemon-juice.jpg"},{"id":11583,"name":"vegetable","localizedName":"vegetable","image":"mixed-vegetables.png"},{"id":14106,"name":"white wine","localizedName":"white wine","image":"white-wine.jpg"},{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"}],"equipment":[{"id":404765,"name":"aluminum foil","localizedName":"aluminum foil","image":"aluminum-foil.png"}]},{"number":5,"step":"Sprinkle with salt and pepper and gently toss.Top each pile of seasoned vegetables with a piece of fish.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":11583,"name":"vegetable","localizedName":"vegetable","image":"mixed-vegetables.png"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[]},{"number":6,"step":"Sprinkle the fish with some of the reserved lemon zest mixture. Top each fish with 2 slices of lemon.Fold up the foil into an air-tight packet.","ingredients":[{"id":9156,"name":"lemon zest","localizedName":"lemon zest","image":"zest-lemon.jpg"},{"id":9150,"name":"lemon","localizedName":"lemon","image":"lemon.png"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404765,"name":"aluminum foil","localizedName":"aluminum foil","image":"aluminum-foil.png"}]},{"number":7,"step":"Place the foil packets in the oven and bake for 15 to 18 minutes depending on the thickness of the fish.","ingredients":[{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"},{"id":404765,"name":"aluminum foil","localizedName":"aluminum foil","image":"aluminum-foil.png"}],"length":{"number":15,"unit":"minutes"}},{"number":8,"step":"Sprinkle the fish with mint just before serving.","ingredients":[{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"},{"id":2064,"name":"mint","localizedName":"mint","image":"mint.jpg"}],"equipment":[]}]}]
