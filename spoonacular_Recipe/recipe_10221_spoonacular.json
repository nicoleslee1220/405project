ttle.","productMatches":[{"id":496396,"title":"Alsace Willm Cuvee Emile Willm Gewurztraminer","description":"Straw-colored with an aromatic nose both fruit forward (litchi) and floral (rose or violet). In the mouth the attack is supple with notes of mango. A robust wine, fat and powerful. The finish is smooth and silky with notes of roses and spices and the headiness of wallflowers.","price":"$22.99","imageUrl":"https://spoonacular.com/productImages/496396-312x231.jpg","averageRating":0.9400000000000001,"ratingCount":7.0,"score":0.8945454545454546,"link":"https://click.linksynergy.com/deeplink?id=*QCiIS6t4gA&mid=2025&murl=https%3A%2F%2Fwww.wine.com%2Fproduct%2Falsace-willm-cuvee-emile-willm-gewurztraminer-2017%2F645449"}]},"instructions":null,"analyzedInstructions":[],"originalId":null}
