[{"name":"","steps":[{"number":1,"step":"If using Hokkien noodles, rinse with warm water; set aside. If using Chinese-style egg noodles, bring a large pot three-fourths full of water to a boil over high heat.","ingredients":[{"id":98967,"name":"hokkien noodles","localizedName":"hokkien noodles","image":"egg-noodles.jpg"},{"id":20409,"name":"egg noodles","localizedName":"egg noodles","image":"egg-noodles.jpg"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":2,"step":"Add the noodles, stir to loosen the strands, return to a boil and cook until tender, about 1 minute.","ingredients":[{"id":20420,"name":"pasta","localizedName":"pasta","image":"fusilli.jpg"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":3,"step":"Drain, rinse thoroughly with cold running water and drain again; set aside.In a nonstick wok over high heat, warm 2 Tbs. of the oil.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404666,"name":"wok","localizedName":"wok","image":"wok.png"}]},{"number":4,"step":"Add the garlic and yellow onion, and toss and stir until lightly browned, about 2 minutes.","ingredients":[{"id":10511282,"name":"yellow onion","localizedName":"yellow onion","image":"brown-onion.png"},{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"}],"equipment":[],"length":{"number":2,"unit":"minutes"}},{"number":5,"step":"Add the shrimp and toss and stir until bright orange, about 1 minute.","ingredients":[{"id":9200,"name":"orange","localizedName":"orange","image":"orange.png"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":6,"step":"Add the carrot, cabbage and green onions, and toss and stir until the vegetables begin to wilt, about 1 minute.","ingredients":[{"id":11291,"name":"green onions","localizedName":"green onions","image":"spring-onions.jpg"},{"id":11583,"name":"vegetable","localizedName":"vegetable","image":"mixed-vegetables.png"},{"id":11109,"name":"cabbage","localizedName":"cabbage","image":"cabbage.jpg"},{"id":11124,"name":"carrot","localizedName":"carrot","image":"sliced-carrot.png"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":7,"step":"Add the tomatoes and potato, and toss and stir, being careful not to break up the cubes, until heated, 1 to 2 minutes.","ingredients":[{"id":11529,"name":"tomato","localizedName":"tomato","image":"tomato.png"},{"id":11352,"name":"potato","localizedName":"potato","image":"potatoes-yukon-gold.png"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":8,"step":"Transfer to a plate and set aside.Return the wok to high heat and add the remaining 1 Tbs. oil.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404666,"name":"wok","localizedName":"wok","image":"wok.png"}]},{"number":9,"step":"Add the noodles and toss to lightly coat with the oil.","ingredients":[{"id":20420,"name":"pasta","localizedName":"pasta","image":"fusilli.jpg"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[]},{"number":10,"step":"Add the ketchup, chili sauce, kecap manis, soy sauce and sugar and toss to thoroughly coat the noodles, about 2 minutes.Push the noodles up the side of the wok to make a well in the middle. Crack the eggs into the well and lightly beat. Cook, without stirring, until set, about 1 minute. Gently fold the eggs into the noodles.","ingredients":[{"id":6972,"name":"chili sauce","localizedName":"chili sauce","image":"tomato-sauce-or-pasta-sauce.jpg"},{"id":10016124,"name":"kecap manis","localizedName":"kecap manis","image":"soy-sauce.jpg"},{"id":16124,"name":"soy sauce","localizedName":"soy sauce","image":"soy-sauce.jpg"},{"id":11935,"name":"ketchup","localizedName":"ketchup","image":"ketchup.png"},{"id":20420,"name":"pasta","localizedName":"pasta","image":"fusilli.jpg"},{"id":19335,"name":"sugar","localizedName":"sugar","image":"sugar-in-bowl.png"},{"id":1123,"name":"egg","localizedName":"egg","image":"egg.png"}],"equipment":[{"id":404666,"name":"wok","localizedName":"wok","image":"wok.png"}],"length":{"number":3,"unit":"minutes"}},{"number":11,"step":"Add the reserved shrimp and vegetables and stir to combine.Divide among individual bowls. Top with the green chili and cucumber slices and accompany with the lemon wedges.","ingredients":[{"id":1029150,"name":"lemon wedge","localizedName":"lemon wedge","image":"lemon-wedge.png"},{"id":31015,"name":"green chili pepper","localizedName":"green chili pepper","image":"chili-peppers-green.jpg"},{"id":11583,"name":"vegetable","localizedName":"vegetable","image":"mixed-vegetables.png"},{"id":11206,"name":"cucumber","localizedName":"cucumber","image":"cucumber.jpg"},{"id":15270,"name":"shrimp","localizedName":"shrimp","image":"shrimp.png"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]}]}]
