[{"name":"","steps":[{"number":1,"step":"In a small bowl, combine the olive oil, garlic, serranos, paprika and 1 teaspoon of pepper.","ingredients":[{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"},{"id":2028,"name":"paprika","localizedName":"paprika","image":"paprika.jpg"},{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"},{"id":1002030,"name":"pepper","localizedName":"pepper","image":"pepper.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":2,"step":"Put the snapper fillets in a large glass dish and rub with the marinade. Cover and refrigerate for at least 30 minutes or up to 4 hours.","ingredients":[{"id":15101,"name":"snapper fillets","localizedName":"snapper fillets","image":"snapper.jpg"},{"id":0,"name":"marinade","localizedName":"marinade","image":"seasoning.png"},{"id":1012034,"name":"dry seasoning rub","localizedName":"dry seasoning rub","image":"seasoning.png"}],"equipment":[],"length":{"number":270,"unit":"minutes"}},{"number":3,"step":"In a large bowl, toss the green and red cabbage with the red onion. In a small bowl, mix the yogurt and mayonnaise.","ingredients":[{"id":11112,"name":"red cabbage","localizedName":"red cabbage","image":"red-cabbage.png"},{"id":4025,"name":"mayonnaise","localizedName":"mayonnaise","image":"mayonnaise.png"},{"id":10011282,"name":"red onion","localizedName":"red onion","image":"red-onion.png"},{"id":1116,"name":"yogurt","localizedName":"yogurt","image":"plain-yogurt.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":4,"step":"Pour over the cabbage, season with salt and pepper and toss. Cover the coleslaw and refrigerate.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":11109,"name":"cabbage","localizedName":"cabbage","image":"cabbage.jpg"}],"equipment":[]},{"number":5,"step":"In a food processor, puree the avocados with the lime juice. Blend in the sour cream. Scrape the avocado cream into a bowl and season with salt and pepper.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":9160,"name":"lime juice","localizedName":"lime juice","image":"lime-juice.png"},{"id":1056,"name":"sour cream","localizedName":"sour cream","image":"sour-cream.jpg"},{"id":9037,"name":"avocado","localizedName":"avocado","image":"avocado.jpg"},{"id":1053,"name":"cream","localizedName":"cream","image":"fluid-cream.jpg"}],"equipment":[{"id":404771,"name":"food processor","localizedName":"food processor","image":"food-processor.png"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":6,"step":"Light a grill. Season the snapper fillets with salt and pepper. Grill the fish over a moderately hot fire for about 3 minutes per side, or until just cooked through.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":15101,"name":"snapper fillets","localizedName":"snapper fillets","image":"snapper.jpg"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[{"id":404706,"name":"grill","localizedName":"grill","image":"grill.jpg"}],"length":{"number":3,"unit":"minutes"}},{"number":7,"step":"Transfer the fish to a platter, break the fish into large chunks and cover loosely with foil. Wrap 3 stacks of 12 tortillas in foil and place on the grill until heated through, about 5 minutes.","ingredients":[{"id":18364,"name":"tortilla","localizedName":"tortilla","image":"flour-tortilla.jpg"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"},{"id":10018364,"name":"wrap","localizedName":"wrap","image":"flour-tortilla.jpg"}],"equipment":[{"id":404706,"name":"grill","localizedName":"grill","image":"grill.jpg"},{"id":404765,"name":"aluminum foil","localizedName":"al