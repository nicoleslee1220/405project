[{"name":"","steps":[{"number":1,"step":"Place beets in a microwave-safe bowl; add enough water to come halfway up sides of bowl. Cover with plastic wrap; vent. Microwave at HIGH 8 minutes or until tender; drain and cool. Peel and slice into wedges.","ingredients":[{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":10018364,"name":"wrap","localizedName":"wrap","image":"flour-tortilla.jpg"}],"equipment":[{"id":404730,"name":"plastic wrap","localizedName":"plastic wrap","image":"plastic-wrap.jpg"},{"id":404762,"name":"microwave","localizedName":"microwave","image":"microwave.jpg"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}],"length":{"number":8,"unit":"minutes"}},{"number":2,"step":"While beets cook, combine arugula and Orange Vinaigrette, tossing gently to coat. Arrange arugula mixture evenly on each of 4 plates. Top evenly with beef, beets, cheese, and nuts.","ingredients":[{"id":4135,"name":"vinaigrette","localizedName":"vinaigrette","image":"vinaigrette.jpg"},{"id":11959,"name":"arugula","localizedName":"arugula","image":"arugula-or-rocket-salad.jpg"},{"id":1041009,"name":"cheese","localizedName":"cheese","image":"cheddar-cheese.png"},{"id":9200,"name":"orange","localizedName":"orange","image":"orange.png"},{"id":11080,"name":"beet","localizedName":"beet","image":"beets.jpg"},{"id":23572,"name":"beef","localizedName":"beef","image":"beef-cubes-raw.png"},{"id":12135,"name":"nuts","localizedName":"nuts","image":"nuts-mixed.jpg"}],"equipment":[]}]}]
