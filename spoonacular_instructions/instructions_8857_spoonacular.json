[{"name":"","steps":[{"number":1,"step":"Fill sink with ice water. In a stockpot, combine onion, carrot, bay leaves, garlic, ginger, zest, and peppercorns with 8 cups water; cover and bring to a boil.","ingredients":[{"id":1022030,"name":"peppercorns","localizedName":"peppercorns","image":"black-pepper.png"},{"id":2004,"name":"bay leaves","localizedName":"bay leaves","image":"bay-leaves.jpg"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"},{"id":11124,"name":"carrot","localizedName":"carrot","image":"sliced-carrot.png"},{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"},{"id":11216,"name":"ginger","localizedName":"ginger","image":"ginger.png"},{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":2,"step":"Add lobsters, cover, and cook 8 minutes once water has returned to a boil.","ingredients":[{"id":15147,"name":"lobster","localizedName":"lobster","image":"lobster.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[],"length":{"number":8,"unit":"minutes"}},{"number":3,"step":"Remove lobsters, reserving the liquid; transfer to the ice bath.","ingredients":[{"id":15147,"name":"lobster","localizedName":"lobster","image":"lobster.png"},{"id":10014412,"name":"ice","localizedName":"ice","image":"ice-cubes.png"}],"equipment":[]},{"number":4,"step":"When lobsters are cool enough to handle, break tails and claws from body; crack open to remove meat. Set meat aside.","ingredients":[{"id":15147,"name":"lobster","localizedName":"lobster","image":"lobster.png"},{"id":1065062,"name":"meat","localizedName":"meat","image":"whole-chicken.jpg"}],"equipment":[]},{"number":5,"step":"Add bodies and empty shells to reserved liquid; return to a boil. Reduce heat; simmer 30 minutes.","ingredients":[{"id":11020420,"name":"pasta shells","localizedName":"pasta shells","image":"shell-