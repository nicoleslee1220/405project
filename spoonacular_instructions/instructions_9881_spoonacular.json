[{"name":"","steps":[{"number":1,"step":"Bring a large pot of water to a rolling boil over high heat.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":2,"step":"Add the lobsters head first and cook for 5 minutes. Using tongs, transfer the lobsters to a rimmed baking sheet. Bring the water back to a boil.","ingredients":[{"id":15147,"name":"lobster","localizedName":"lobster","image":"lobster.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404727,"name":"baking sheet","localizedName":"baking sheet","image":"baking-sheet.jpg"},{"id":404641,"name":"tongs","localizedName":"tongs","image":"tongs.jpg"}],"length":{"number":5,"unit":"minutes"}},{"number":3,"step":"Add the bok choy and cook for 30 seconds.","ingredients":[{"id":11116,"name":"bok choy","localizedName":"bok choy","image":"bok-choy.jpg"}],"equipment":[]},{"number":4,"step":"Transfer the bok choy to a work surface and quarter each head lengthwise.","ingredients":[{"id":11116,"name":"bok choy","localizedName":"bok choy","image":"bok-choy.jpg"}],"equipment":[]},{"number":5,"step":"Add the udon noodles to the boiling water and cook, stirring often, until al dente, about 6 minutes.","ingredients":[{"id":98966,"name":"udon noodles","localizedName":"udon noodles","image":"dry-udon-noodles.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[],"length":{"number":6,"unit":"minutes"}},{"number":6,"step":"Drain the udon. Return the noodles to the pot and cover with cold water.","ingredients":[{"id":20420,"name":"pasta","localizedName":"pasta","image":"fusilli.jpg"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":7,"step":"Drain and rinse the udon 2 more times, then drain thoroughly, lifting the noodles a few times to help them dry.","ingredients":[{"id":20420,"name":"pasta","localizedName":"pasta","image":"fusilli.jpg"}],"equipment":[]},{"number":8,"step":"In a small skillet, heat 1/4 cup of the oil.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":9,"step":"Add the shallots and cook over moderate heat, stirring a few times, until browned and crisp, about 3 minutes. Using a slotted spoon, transfer the shallots to paper towels to drain, then salt lightly.","ingredients":[{"id":11677,"name":"shallot","localizedName":"shallot","image":"shallots.jpg"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[{"id":404636,"name":"slotted spoon","localizedName":"slotted spoon","image":"slotted-spoon.jpg"},{"id":405895,"name":"paper towels","localizedName":"paper towels","image":"paper-towels.jpg"}],"length":{"number":3,"unit":"minutes"}},{"number":10,"step":"Crack the lobster claws and remove the meat in one piece. Twist off the lobster tails. Using kitchen shears, slit one side of each tail shell and remove the meat in one piece.","ingredients":[{"id":10015147,"name":"lobster tails","localizedName":"lobster tails","image":"lobster-tails.jpg"},{"id":15147,"name":"lobster","localizedName":"lobster","image":"lobster.png"},{"id":1065062,"name":"meat","localizedName":"meat","image":"whole-chicken.jpg"}],"equipment":[]},{"number":11,"step":"Cut the tail meat in half lengthwise and discard the intestinal veins.","ingredients":[{"id":1065062,"name":"meat","localizedName":"meat","image":"whole-chicken.jpg"}],"equipment":[]},{"number":12,"step":"Using a sharp knife, peel the orange, taking care to remove all of the bitter white pith.","ingredients":[{"id":9200,"name":"orange","localizedName":"orange","image":"orange.png"}],"equipment":[{"id":404745,"name":"knife","localizedName":"knife","image":"chefs-knife.jpg"}]},{"number":13,"step":"Cut in between the membranes to release the sections.","ingredients":[],"equipment":[]},{"number":14,"step":"In a large skillet, heat the remaining 3 tablespoons of oil.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":15,"step":"Add the onion and mushrooms and cook over high heat, stirring occasionally, until browned, about 5 minutes.","ingredients":[{"id":11260,"name":"mushrooms","localizedName":"mushrooms","image":"mushrooms.png"},{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":16,"step":"Add the brown sugar, bok choy, vegetable stock and sake and bring to a simmer.","ingredients":[{"id":6615,"name":"vegetable stock","localizedName":"vegetable stock","image":"chicken-broth.png"},{"id":19334,"name":"brown sugar","localizedName":"brown sugar","image":"dark-brown-sugar.png"},{"id":11116,"name":"bok choy","localizedName":"bok choy","image":"bok-choy.jpg"},{"id":43479,"name":"sake","localizedName":"sake","image":"sake.png"}],"equipment":[]},{"number":17,"step":"Add the udon, season with salt and pepper and stir well.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"}],"equipment":[]},{"number":18,"step":"In a medium skillet, melt the butter.","ingredients":[{"id":1001,"name":"butter","localizedName":"butter","image":"butter-sliced.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":19,"step":"Add the lobster meat and cook over moderately low heat, turning a few times, until warmed through, about 2 minutes. Crush the fried shallots over the lobster and stir to coat. Season with salt and pepper.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":10115147,"name":"lobster meat","localizedName":"lobster meat","image":"lobster.png"},{"id":11677,"name":"shallot","localizedName":"shallot","image":"shallots.jpg"},{"id":15147,"name":"lobster","localizedName":"lobster","image":"lobster.png"}],"equipment":[],"length":{"number":2,"unit":"minutes"}},{"number":20,"step":"Transfer the noodles and vegetables to a serving bowl or deep plates, set the lobster and orange sections on top and serve right away.","ingredients":[{"id":1019200,"name":"orange slices","localizedName":"orange slices","image":"orange-slices.png"},{"id":11583,"name":"vegetable","localizedName":"vegetable","image":"mixed-vegetables.png"},{"id":15147,"name":"lobster","localizedName":"lobster","image":"lobster.png"},{"id":20420,"name":"pasta","localizedName":"pasta","image":"fusilli.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]}]}]
