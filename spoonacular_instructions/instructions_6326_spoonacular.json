[{"name":"","steps":[{"number":1,"step":"Pour the water into a large stockpot.","ingredients":[{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":2,"step":"Add the fish heads and bones, 3 tablespoons of the fish sauce and the lemongrass, ginger, onion and 2 teaspoons of kosher salt. Bring to a boil. Reduce the heat to moderately low and simmer for 1 hour.","ingredients":[{"id":1082047,"name":"kosher salt","localizedName":"kosher salt","image":"salt.jpg"},{"id":6179,"name":"fish sauce","localizedName":"fish sauce","image":"asian-fish-sauce.jpg"},{"id":11972,"name":"lemon grass","localizedName":"lemon grass","image":"lemongrass.png"},{"id":11216,"name":"ginger","localizedName":"ginger","image":"ginger.png"},{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[],"length":{"number":60,"unit":"minutes"}},{"number":3,"step":"Meanwhile, cut the snapper fillets into 1 1/2-inch pieces and transfer to a medium bowl.","ingredients":[{"id":15101,"name":"snapper fillets","localizedName":"snapper fillets","image":"snapper.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":4,"step":"Add 2 tablespoons of the oil, the remaining 2 tablespoons of fish sauce and toss. Refrigerate the fish for 1 hour.","ingredients":[{"id":6179,"name":"fish sauce","localizedName":"fish sauce","image":"asian-fish-sauce.jpg"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[],"length":{"number":60,"unit":"minutes"}},{"number":5,"step":"Heat the remaining 6 tablespoons of oil in a small skillet.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":6,"step":"Add the garlic and cook over high heat, stirring, until fragrant and lightly golden, about 30 seconds.","ingredients":[{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"}],"equipment":[]},{"number":7,"step":"Pour the garlic oil into a bowl and let cool.","ingredients":[{"id":1004053,"name":"garlic oil","localizedName":"garlic oil","image":"garlic-oil.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":8,"step":"Strain the soup through a fine sieve, pressing hard on the solids. Wipe out the pot. Return the fish soup to it and bring to a boil.","ingredients":[{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"},{"id":0,"name":"soup","localizedName":"soup","image":""}],"equipment":[{"id":405600,"name":"sieve","localizedName":"sieve","image":"strainer.png"},{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":9,"step":"Add the mushrooms and simmer over moderate heat until just tender, about 1 minute.","ingredients":[{"id":11260,"name":"mushrooms","localizedName":"mushrooms","image":"mushrooms.png"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":10,"step":"Add the marinated snapper and cook just until opaque, about 2 minutes. Season the soup with salt.","ingredients":[{"id":15101,"name":"snapper","localizedName":"snapper","image":"snapper.jpg"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"},{"id":0,"name":"soup","localizedName":"soup","image":""}],"equipment":[],"length":{"number":2,"unit":"minutes"}},{"number":11,"step":"Add the bean sprouts and scallions and stir in the garlic oil.","ingredients":[{"id":11043,"name":"bean sprouts","localizedName":"bean sprouts","image":"bean-sprouts.jpg"},{"id":1004053,"name":"garlic oil","localizedName":"garlic oil","image":"garlic-oil.jpg"}