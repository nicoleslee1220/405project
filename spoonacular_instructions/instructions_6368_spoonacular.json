[{"name":"","steps":[{"number":1,"step":"Using scissors, cut the cellophane noodles into 3-inch lengths. In a heatproof bowl, cover the noodles with very hot tap water and let stand until clear and pliable, about 5 minutes; drain.","ingredients":[{"id":16082,"name":"cellophane noodles","localizedName":"cellophane noodles","image":"dry-cellophane-noodles.png"},{"id":20420,"name":"pasta","localizedName":"pasta","image":"fusilli.jpg"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404673,"name":"kitchen scissors","localizedName":"kitchen scissors","image":"kitchen-scissors.jpg"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}],"length":{"number":5,"unit":"minutes"}},{"number":2,"step":"In a large saucepan, bring the stock, scallions, mustard-green pickling liquid, ginger and 1 tablespoon each of rice vinegar and fish sauce to a boil. Simmer over moderately low heat for 15 minutes.","ingredients":[{"id":1022053,"name":"rice vinegar","localizedName":"rice vinegar","image":"rice-vinegar.png"},{"id":6179,"name":"fish sauce","localizedName":"fish sauce","image":"asian-fish-sauce.jpg"},{"id":11291,"name":"green onions","localizedName":"green onions","image":"spring-onions.jpg"},{"id":2046,"name":"mustard","localizedName":"mustard","image":"regular-mustard.jpg"},{"id":11216,"name":"ginger","localizedName":"ginger","image":"ginger.png"},{"id":1006615,"name":"stock","localizedName":"stock","image":"chicken-broth.png"}],"equipment":[{"id":404669,"name":"sauce pan","localizedName":"sauce pan","image":"sauce-pan.jpg"}],"length":{"number":15,"unit":"minutes"}},{"number":3,"step":"Add the cabbage and simmer over moderate heat for 10 minutes.","ingredients":[{"id":11109,"name":"cabbage","localizedName":"cabbage","image":"cabbage.jpg"}],"equipment":[],"length":{"number":10,"unit":"minutes"}},{"number":4,"step":"Add the enoki mushrooms and cellophane noodles and simmer for 3 minutes.","ingredients":[{"id":16082,"name":"