[{"name":"","steps":[{"number":1,"step":"In a large bowl, mix ground turkey, five spice, chili flakes, and salt. Shape mixture into 1-inch balls.","ingredients":[{"id":5662,"name":"ground turkey","localizedName":"ground turkey","image":"meat-ground.jpg"},{"id":1032009,"name":"red pepper flakes","localizedName":"red pepper flakes","image":"red-pepper-flakes.jpg"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}]},{"number":2,"step":"Pour flour onto a large rimmed plate and roll balls in flour to coat lightly.","ingredients":[{"id":20081,"name":"all purpose flour","localizedName":"all purpose flour","image":"flour.png"},{"id":0,"name":"roll","localizedName":"roll","image":"dinner-yeast-rolls.jpg"}],"equipment":[]},{"number":3,"step":"Pour oil into a 4- to 5-quart nonstick pan over medium-high heat.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":4,"step":"Add meatballs in a single layer. Cook, turning frequently, until lightly browned all over and barely pink in the center (cut to test), 5 to 7 minutes. Using a slotted spoon, transfer to a plate.","ingredients":[{"id":10110219,"name":"meatballs","localizedName":"meatballs","image":"meatballs.png"}],"equipment":[{"id":404636,"name":"slotted spoon","localizedName":"slotted spoon","image":"slotted-spoon.jpg"}],"length":{"number":5,"unit":"minutes"}},{"number":5,"step":"Add garlic and ginger to pan and stir just until fragrant, about 30 seconds.","ingredients":[{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"},{"id":11216,"name":"ginger","localizedName":"ginger","image":"ginger.png"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":6,"step":"Add coconut milk, broth, and meatballs; bring to a simmer. Adjust heat to maintain a simmer, cover, and cook until meatballs are no longer pink in the center, about 5 minutes.","ingredients":[{"id":12118,"name":"coconut milk","localizedName":"coconut milk","image":"coconut-milk.png"},{"id":10110219,"name":"meatballs","localizedName":"meatballs","image":"meatballs.png"},{"id":1006615,"name":"broth","localizedName":"broth","image":"chicken-broth.png"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":7,"step":"Stir in bok choy, baby corn, tomatoes, water chestnuts, green onions, and lime juice and cook, stirring often, until bok choy leaves are wilted and stems are barely tender to bite, about 5 minutes.","ingredients":[{"id":10011116,"name":"bok choy leaves","localizedName":"bok choy leaves","image":"bok-choy.jpg"},{"id":11590,"name":"water chestnuts","localizedName":"water chestnuts","image":"water-chestnuts.png"},{"id":11291,"name":"green onions","localizedName":"green onions","image":"spring-onions.jpg"},{"id":9160,"name":"lime juice","localizedName":"lime juice","image":"lime-juice.png"},{"id":10011168,"name":"baby corn","localizedName":"baby corn","image":"baby-corn.jpg"},{"id":11116,"name":"bok choy","localizedName":"bok choy","image":"bok-choy.jpg"},{"id":11529,"name":"tomato","localizedName":"tomato","image":"tomato.png"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":8,"step":"Sprinkle with cilantro and serve from pan.","ingredients":[{"id":11165,"name":"cilantro","localizedName":"cilantro","image":"cilantro.png"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]}]}]
