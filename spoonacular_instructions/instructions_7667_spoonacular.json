[{"name":"","steps":[{"number":1,"step":"Watch how to make this recipe.","ingredients":[],"equipment":[]},{"number":2,"step":"Saute the olive oil, onions, and ginger in a large saute pan over medium-low heat for 10 minutes, or until the onions are translucent.","ingredients":[{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"},{"id":11216,"name":"ginger","localizedName":"ginger","image":"ginger.png"},{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":10,"unit":"minutes"}},{"number":3,"step":"Add the garlic and cook for 1 more minute.","ingredients":[{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":4,"step":"Add the mangos, reduce the heat to low and cook for 10 more minutes.","ingredients":[{"id":9176,"name":"mango","localizedName":"mango","image":"mango.jpg"}],"equipment":[],"length":{"number":10,"unit":"minutes"}},{"number":5,"step":"Add the orange juice, brown sugar, salt, black pepper, and jalapeno; cook for 10 more minutes, until orange juice is reduced, stirring occasionally.","ingredients":[{"id":1002030,"name":"black pepper","localizedName":"black pepper","image":"pepper.jpg"},{"id":9206,"name":"orange juice","localizedName":"orange juice","image":"orange-juice.jpg"},{"id":19334,"name":"brown sugar","localizedName":"brown sugar","image":"dark-brown-sugar.png"},{"id":11979,"name":"jalapeno pepper","localizedName":"jalapeno pepper","image":"jalapeno-pepper.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[],"length":{"number":10,"unit":"minutes"}},{"number":6,"step":"Remove from the heat and add the mint.","ingredients":[{"id":2064,"name":"mint","localizedName":"mint","image":"mint.jpg"}],"equipment":[]},{"number":7,"step":"Serve warm, at room temperature, or chi