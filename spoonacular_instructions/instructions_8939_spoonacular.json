[{"name":"","steps":[{"number":1,"step":"To poach chicken breast: Cover chicken breast in broth in a small pot, add water if it doesnt cover the chicken.","ingredients":[{"id":5062,"name":"chicken breast","localizedName":"chicken breast","image":"chicken-breasts.png"},{"id":5006,"name":"whole chicken","localizedName":"whole chicken","image":"whole-chicken.jpg"},{"id":1006615,"name":"broth","localizedName":"broth","image":"chicken-broth.png"},{"id":14412,"name":"water","localizedName":"water","image":"water.png"}],"equipment":[{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":2,"step":"Add salt and pepper, a piece of celery and its leaves (you could add herbs like parsley, garlic, onion, or whatever you want) and bring to a boil. Reduce to a simmer and cook 5 minutes.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":11297,"name":"parsley","localizedName":"parsley","image":"parsley.jpg"},{"id":11143,"name":"celery","localizedName":"celery","image":"celery.jpg"},{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"},{"id":1002044,"name":"herbs","localizedName":"herbs","image":"mixed-fresh-herbs.jpg"},{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":3,"step":"Remove from heat, cover tight and let it sit for 15-20 minutes or until thickest part of the breast registers 160 degrees. Chicken will be cooked through.","ingredients":[{"id":5006,"name":"whole chicken","localizedName":"whole chicken","image":"whole-chicken.jpg"}],"equipment":[],"length":{"number":20,"unit":"minutes"}},{"number":4,"step":"Let it cool and cut into small cubes.","ingredients":[],"equipment":[]},{"number":5,"step":"Combine mayo, yogurt, salt, pepper and mix well.","ingredients":[{"id":1002030,"name":"pepper","localizedName":"pepper","image":"pepper.jpg"},{"id":1116,"name":"yogurt","localizedName":"yogurt","image":"plain-yogurt.jpg"},{"id":4025,"name":"mayonnaise","localizedName":"mayonnaise","image":"mayonnaise.png"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[]},{"number":6,"step":"Add chicken, grapes, apples, celery, and let it chill in the refrigerator until you are ready to eat it.","ingredients":[{"id":5006,"name":"whole chicken","localizedName":"whole chicken","image":"whole-chicken.jpg"},{"id":9003,"name":"apple","localizedName":"apple","image":"apple.jpg"},{"id":11143,"name":"celery","localizedName":"celery","image":"celery.jpg"},{"id":9132,"name":"grapes","localizedName":"grapes","image":"red-grapes.jpg"}],"equipment":[]},{"number":7,"step":"Mix in pecans right before serving.","ingredients":[{"id":12142,"name":"pecans","localizedName":"pecans","image":"pecans.jpg"}],"equipment":[]},{"number":8,"step":"Serve over baby greens.","ingredients":[{"id":21052,"name":"mixed baby greens","localizedName":"mixed baby greens","image":"mixed-greens-or-mesclun.jpg"}],"equipment":[]}]}]
