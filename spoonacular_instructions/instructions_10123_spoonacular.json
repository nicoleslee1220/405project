[{"name":"","steps":[{"number":1,"step":"Heat 1 tablespoon of vegetable oil in a wok or large skillet over high heat. Stir in the mushrooms, snow peas, water chestnuts, and bok choy; season to taste with salt and pepper. Cook and stir until the vegetables are just tender, about 5 minutes.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":11590,"name":"water chestnuts","localizedName":"water chestnuts","image":"water-chestnuts.png"},{"id":4669,"name":"vegetable oil","localizedName":"vegetable oil","image":"vegetable-oil.jpg"},{"id":11583,"name":"vegetable","localizedName":"vegetable","image":"mixed-vegetables.png"},{"id":11260,"name":"mushrooms","localizedName":"mushrooms","image":"mushrooms.png"},{"id":11300,"name":"snow peas","localizedName":"snow peas","image":"snow-peas.jpg"},{"id":11116,"name":"bok choy","localizedName":"bok choy","image":"bok-choy.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"},{"id":404666,"name":"wok","localizedName":"wok","image":"wok.png"}],"length":{"number":5,"unit":"minutes"}},{"number":2,"step":"Remove the vegetables from the wok and wipe the wok clean.","ingredients":[{"id":11583,"name":"vegetable","localizedName":"vegetable","image":"mixed-vegetables.png"}],"equipment":[{"id":404666,"name":"wok","localizedName":"wok","image":"wok.png"}]},{"number":3,"step":"Heat the remaining 1 tablespoon of vegetable oil in the wok. Stir in the garlic and ginger; cook a few seconds until the garlic begins to turn golden brown. Stir in the chicken and cook until the chicken is no longer pink, about 5 minutes.","ingredients":[{"id":4669,"name":"vegetable oil","localizedName":"vegetable oil","image":"vegetable-oil.jpg"},{"id":5006,"name":"whole chicken","localizedName":"whole chicken","image":"whole-chicken.jpg"},{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"},{"id":11216,"name":"ginger","localizedNa