[{"name":"","steps":[{"number":1,"step":"Preheat the oven to 40","ingredients":[],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"}]},{"number":2,"step":"Using 1/2 tablespoon of the oil, coat the bottom of a medium ovenproof skillet. In a microwave-safe bowl, pour the Madeira over the raisins. Microwave at high power for 1 minute, or until the raisins are plump.","ingredients":[{"id":10014106,"name":"madeira","localizedName":"madeira","image":"red-wine.jpg"},{"id":9299,"name":"raisins","localizedName":"raisins","image":"raisins.jpg"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404762,"name":"microwave","localizedName":"microwave","image":"microwave.jpg"},{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"},{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"}],"length":{"number":1,"unit":"minutes"}},{"number":3,"step":"Let cool, then drain the raisins, reserving the Madeira.","ingredients":[{"id":10014106,"name":"madeira","localizedName":"madeira","image":"red-wine.jpg"},{"id":9299,"name":"raisins","localizedName":"raisins","image":"raisins.jpg"}],"equipment":[]},{"number":4,"step":"Combine the raisins with the chopped sage and a pinch each of salt and pepper. Stuff the quail with the raisins and season with salt and pepper.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":9299,"name":"raisins","localizedName":"raisins","image":"raisins.jpg"},{"id":5157,"name":"quail","localizedName":"quail","image":"quail.png"},{"id":99226,"name":"sage","localizedName":"sage","image":"fresh-sage.png"}],"equipment":[]},{"number":5,"step":"Transfer the quail to the skillet and turn to coat them with oil. Roast for 15 minutes.","ingredients":[{"id":5157,"name":"quail","localizedName":"quail","image":"quail.png"},{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":15,"unit":"minutes"}},{"number":6,"step":"Meanwhile, heat the remaining 1 tablespoon of oil in a large skillet.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":7,"step":"Add the onion and cook over moderately high heat, stirring, until lightly browned, about 5 minutes.","ingredients":[{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":8,"step":"Add the cabbage, caraway seeds and bay leaf and cook, stirring frequently, until the cabbage is tender, about 8 minutes longer. Stir in the reserved Madeira and any accumulated juices from the quail and cook until evaporated, about 1 minute. Discard the bay leaf. Spoon the cabbage onto plates and top with the quail.","ingredients":[{"id":2005,"name":"caraway seeds","localizedName":"caraway seeds","image":"caraway-seeds.jpg"},{"id":2004,"name":"bay leaves","localizedName":"bay leaves","image":"bay-leaves.jpg"},{"id":11109,"name":"cabbage","localizedName":"cabbage","image":"cabbage.jpg"},{"id":10014106,"name":"madeira","localizedName":"madeira","image":"red-wine.jpg"},{"id":5157,"name":"quail","localizedName":"quail","image":"quail.png"}],"equipment":[],"length":{"number":9,"unit":"minutes"}},{"number":9,"step":"Garnish with the sage sprigs and serve.","ingredients":[{"id":99226,"name":"sage","localizedName":"sage","image":"fresh-sage.png"}],"equipment":[]}]}]
