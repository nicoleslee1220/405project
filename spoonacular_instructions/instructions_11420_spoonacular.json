[{"name":"","steps":[{"number":1,"step":"Heat 1 tablespoon of the olive oil in a large, deep nonstick skillet.","ingredients":[{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":2,"step":"Add the pancetta and cook over moderate heat, turning once, until golden, about 5 minutes.","ingredients":[{"id":10410123,"name":"pancetta","localizedName":"pancetta","image":"pancetta.png"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":3,"step":"Transfer the pancetta to a cutting board and coarsely chop. Strain the fat through a fine sieve; wipe out the skillet and pour the fat back in.","ingredients":[{"id":10410123,"name":"pancetta","localizedName":"pancetta","image":"pancetta.png"}],"equipment":[{"id":404716,"name":"cutting board","localizedName":"cutting board","image":"cutting-board.jpg"},{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"},{"id":405600,"name":"sieve","localizedName":"sieve","image":"strainer.png"}]},{"number":4,"step":"Add the pine nuts to the skillet and cook over moderate heat, stirring, until golden, about 3 minutes. Using a slotted spoon, transfer the nuts to a plate.","ingredients":[{"id":12147,"name":"pine nuts","localizedName":"pine nuts","image":"pine-nuts.png"},{"id":12135,"name":"nuts","localizedName":"nuts","image":"nuts-mixed.jpg"}],"equipment":[{"id":404636,"name":"slotted spoon","localizedName":"slotted spoon","image":"slotted-spoon.jpg"},{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}],"length":{"number":3,"unit":"minutes"}},{"number":5,"step":"Add the thyme and garlic to the skillet and cook, stirring, until fragrant, about 1 minute.","ingredients":[{"id":11215,"name":"garlic","localizedName":"garlic","image":"garlic.png"},{"id":2049,"name":"thyme","localizedName":"thyme","image":"thyme.jpg"}],"equipment":[{"id":404645,"name":