[{"name":"","steps":[{"number":1,"step":"Preheat oven to 500 degrees.Toss cubed zucchini, squash, and mushrooms in a bowl with olive oil and a sprinkling of kosher salt. Arrange onto two sheet pans (to avoid crowding) and roast in the hot oven for 5 to 10 minutes, or until brown and black parts begin to show.","ingredients":[{"id":1082047,"name":"kosher salt","localizedName":"kosher salt","image":"salt.jpg"},{"id":11260,"name":"mushrooms","localizedName":"mushrooms","image":"mushrooms.png"},{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"},{"id":11477,"name":"zucchini","localizedName":"zucchini","image":"zucchini.jpg"},{"id":10011485,"name":"squash","localizedName":"squash","image":"butternut-squash.jpg"}],"equipment":[{"id":404783,"name":"bowl","localizedName":"bowl","image":"bowl.jpg"},{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"}],"length":{"number":5,"unit":"minutes"}},{"number":2,"step":"Remove from oven and set aside.In a heavy pot, heat olive oil over medium heat.","ingredients":[{"id":4053,"name":"olive oil","localizedName":"olive oil","image":"olive-oil.jpg"}],"equipment":[{"id":404784,"name":"oven","localizedName":"oven","image":"oven.jpg"},{"id":404752,"name":"pot","localizedName":"pot","image":"stock-pot.jpg"}]},{"number":3,"step":"Add carrots, onions, and celery and stir to cook for 3 minutes.","ingredients":[{"id":11124,"name":"carrot","localizedName":"carrot","image":"sliced-carrot.png"},{"id":11143,"name":"celery","localizedName":"celery","image":"celery.jpg"},{"id":11282,"name":"onion","localizedName":"onion","image":"brown-onion.png"}],"equipment":[],"length":{"number":3,"unit":"minutes"}},{"number":4,"step":"Pour in broth and bring to a boil. Reduce heat to low and simmer for ten minutes.","ingredients":[{"id":1006615,"name":"broth","localizedName":"broth","image":"chicken-broth.png"}],"equipment":[],"length":{"number":10,"unit":"minutes"}},{"number":5,"step":"Add cannelini beans and green beans. Cook on low for five minutes.","ingredients":[{"id":10716050,"name":"cannellini beans","localizedName":"cannellini beans","image":"cooked-cannellini-beans.png"},{"id":11052,"name":"green beans","localizedName":"green beans","image":"green-beans-or-string-beans.jpg"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":6,"step":"Add pasta and cook for an additional five minutes.","ingredients":[{"id":20420,"name":"pasta","localizedName":"pasta","image":"fusilli.jpg"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":7,"step":"Add canned tomatoes, stir, and cook for an additional five minutes on low heat. Check for seasonings, adding salt, pepper, and turmeric.","ingredients":[{"id":10011693,"name":"canned tomatoes","localizedName":"canned tomatoes","image":"tomatoes-canned.png"},{"id":1042027,"name":"seasoning","localizedName":"seasoning","image":"seasoning.png"},{"id":2043,"name":"turmeric","localizedName":"turmeric","image":"turmeric.jpg"},{"id":1002030,"name":"pepper","localizedName":"pepper","image":"pepper.jpg"},{"id":2047,"name":"salt","localizedName":"salt","image":"salt.jpg"}],"equipment":[],"length":{"number":5,"unit":"minutes"}},{"number":8,"step":"Add roasted vegetables at the very end. Stir and allow to simmer and heat for a few minutes before serving. Be sure to check seasoning!","ingredients":[{"id":11583,"name":"vegetable","localizedName":"vegetable","image":"mixed-vegetables.png"},{"id":1042027,"name":"seasoning","localizedName":"seasoning","image":"seasoning.png"}],"equipment":[]},{"number":9,"step":"Add more broth if necessary to increase liquid amount.","ingredients":[{"id":1006615,"name":"broth","localizedName":"broth","image":"chicken-broth.png"}],"equipment":[]},{"number":10,"step":"Serve with Parmesan shavings and crusty Italian bread.","ingredients":[{"id":10028033,"name":"italian bread","localizedName":"italian bread","image":"italian-bread.jpg"},{"id":1033,"name":"parmesan","localizedName":"parmesan","image":"parmesan.jpg"}],"equipment":[]}]}]
