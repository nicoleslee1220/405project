[{"name":"","steps":[{"number":1,"step":"In a food processor, combine pistachios, oregano, thyme, and garlic powder. Process until finely chopped and transfer to a shallow dish. Season both sides of tilapia fillets with salt and pepper.","ingredients":[{"id":1102047,"name":"salt and pepper","localizedName":"salt and pepper","image":"salt-and-pepper.jpg"},{"id":15261,"name":"tilapia fillets","localizedName":"tilapia fillets","image":"raw-tilapia.jpg"},{"id":1022020,"name":"garlic powder","localizedName":"garlic powder","image":"garlic-powder.png"},{"id":12151,"name":"pistachio nuts","localizedName":"pistachio nuts","image":"pistachios.jpg"},{"id":2027,"name":"oregano","localizedName":"oregano","image":"oregano.jpg"},{"id":2049,"name":"thyme","localizedName":"thyme","image":"thyme.jpg"}],"equipment":[{"id":404771,"name":"food processor","localizedName":"food processor","image":"food-processor.png"}]},{"number":2,"step":"Brush honey mustard over both sides of tilapia and then transfer to pistachio mixture. Press mixture into both sides of fish.","ingredients":[{"id":99227,"name":"honey mustard","localizedName":"honey mustard","image":"regular-mustard.jpg"},{"id":12151,"name":"pistachio nuts","localizedName":"pistachio nuts","image":"pistachios.jpg"},{"id":15261,"name":"tilapia","localizedName":"tilapia","image":"raw-tilapia.jpg"},{"id":10115261,"name":"fish","localizedName":"fish","image":"fish-fillet.jpg"}],"equipment":[]},{"number":3,"step":"Heat 1 tablespoon of the oil in a large skillet over medium heat.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":4,"step":"Add tilapia and cook 2 to 3 minutes per side, until fork-tender.","ingredients":[{"id":15261,"name":"tilapia","localizedName":"tilapia","image":"raw-tilapia.jpg"}],"equipment":[],"length":{"number":2,"unit":"minutes"}},{"number":5,"step":"Heat remaining oil in a small skillet over high heat.","ingredients":[{"id":4582,"name":"cooking oil","localizedName":"cooking oil","image":"vegetable-oil.jpg"}],"equipment":[{"id":404645,"name":"frying pan","localizedName":"frying pan","image":"pan.png"}]},{"number":6,"step":"Add prosciutto and cook 2 minutes, until golden brown.","ingredients":[{"id":10010123,"name":"prosciutto","localizedName":"prosciutto","image":"proscuitto.jpg"}],"equipment":[],"length":{"number":2,"unit":"minutes"}},{"number":7,"step":"Add pine nuts and cook 1 minute, until nuts are golden brown.","ingredients":[{"id":12147,"name":"pine nuts","localizedName":"pine nuts","image":"pine-nuts.png"},{"id":12135,"name":"nuts","localizedName":"nuts","image":"nuts-mixed.jpg"}],"equipment":[],"length":{"number":1,"unit":"minutes"}},{"number":8,"step":"Add chard leaves, cover and steam 2 minutes, until leaves soften. Arrange chard mixture on a serving platter and top with Gorgonzola and serve with tilapia.","ingredients":[{"id":1011004,"name":"gorgonzola","localizedName":"gorgonzola","image":"gorgonzola.jpg"},{"id":15261,"name":"tilapia","localizedName":"tilapia","image":"raw-tilapia.jpg"},{"id":11147,"name":"swiss chard","localizedName":"swiss chard","image":"swiss-chard.jpg"}],"equipment":[],"length":{"number":2,"unit":"minutes"}}]}]
